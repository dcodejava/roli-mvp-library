{
  "name":              "dcodejava/roli-mvp-library",
  "homepage":          "https://github.com/dcodejava/roli-mvp-library",
  "description":       "MVP Online Book Library system",
  "type":              "wordpress-plugin",
  "license":           "GPL-3.0+",
  "version":           "0.1",
  "keywords":          [
    "WordPress",
    "plugins",
    "themes",
    "book",
    "skeleton",
    "Roli",
    "library",
    "management",
    "system"
  ],
  "require":           {
    "php":                                 ">=8",
    "composer/installers":                 "~1.0"
  },
  "repositories":      [
    {
      "type": "vcs",
      "url":  "https://github.com/dcodejava/roli-mvp-library"
    }
  ],
  "minimum-stability": "stable",
  "authors":           [
    {
      "name":     "Dickson Okorodudu",
      "email":    "dcodejava@gmail.com",
      "homepage": "https://dcodejava.com",
      "role":     "Developer"
    },
    {
      "name":     "Daniel Dickson",
      "email":    "dcoderoli@gmail.com",
      "homepage": "https://dcodejava.com",
      "role":     "Developer"
    }
  ],
  "support":           {
    "email": "support@dcodejava.com"
  },
  "config":            {
    "platform": {
      "php": "8.1"
    }
  },
  "autoload":          {
    "psr-4": {
      "Roli\\": "src/"
    }
  },
    "scripts": {
      "post-install-cmd": [
        "wp-enforcer"
      ],
      "post-update-cmd":  [
        "wp-enforcer"
      ]
    }
}
